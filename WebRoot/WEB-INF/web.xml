<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd ">
  <display-name></display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <resource-ref>
      <description>dataSource</description>
      <res-ref-name>jdbc/test</res-ref-name>
      <res-type>javax.sql.DataSource</res-type>
      <res-auth>Container</res-auth>
  </resource-ref>
  <filter>
  	<filter-name>LoginFilter</filter-name>
  	<filter-class>filter.LoginFilter</filter-class>
  </filter>
   <filter-mapping>
  	<filter-name>LoginFilter</filter-name>
  	<url-pattern>/manager/mangerInfro/mangerInfro.html</url-pattern>
  </filter-mapping>
  <filter>
  	<filter-name>workerFilter</filter-name>
  	<filter-class>filter.workerFilter</filter-class>
  </filter>
   <filter-mapping>
  	<filter-name>workerFilter</filter-name>
  	<url-pattern>/workPart/worker/register.html</url-pattern>
  </filter-mapping>
</web-app>